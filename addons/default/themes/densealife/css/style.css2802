#body-container .right-body-container{width:274px;float:left;}
#body-container .right-body-container .login-form-block{border:1px solid #d8d8d8;height:200px;width:96%; padding:2%;background:url(../img/login-heading.png);background-repeat:no-repeat; background-position:8px 12px;}
#body-container .right-body-container .login-form-block span{font-size:11px;text-decoration:underline;}
#body-container .right-body-container .login-form-block span.key-icon{display:block;background:url(../img/key-icon.png);background-repeat:no-repeat; background-position:97% 12px; padding:10px;text-indent:-999px;overflow: hidden;}
#body-container .right-body-container .login-form-block input[type=text],  input[type=password]{width:91%;margin:2%;border:1px solid #d8d8d8;line-height:25px; height:25px;padding:2%;-webkit-box-shadow:inset 0 0 20px 5px #eae6e6;box-shadow:inset 0 0 20px 5px #eae6e6;}
.activity-feeds{border:1px solid #d8d8d8;height:auto;width:96%; padding:2%;background:url(../img/activity-icon.png);background-repeat:no-repeat; background-position: -1px 0px;margin: 15px auto;}
.activity-feeds span.heading{padding:10px; text-transform:uppercase;color:#236aa0;font-size: 16px;}
.activity-feeds ul{width:100%;float:left;margin:0px; padding:0px;}
.activity-feeds ul li{border-bottom:1px solid #d8d8d8; min-height:80px;overflow: hidden;}
.activity-feeds ul li span {display:block;float:left;}
.activity-feeds ul li span.text {float:left;padding:5px;font-size: 10px;width: 75%;}
span.name {display:block;margin: 4px auto 0px;padding: 5px;font-weight: bold;}
span.name a{font-size:10px;}
.activity-feeds span.more {display:block; text-align:right; padding:20px 5px 2px;clear: both; color:#236aa0;}
.activity-feeds ul li span img{width:50px; height:50px;background-color:#CCCCCC;margin:10px auto;}
footer{width: 96%;margin: 20px auto;border:1px solid #d8d8d8;padding: 2% 2% 3%;background-color:#ffffff;float:left;}
footer span{display:inline-block; float:left;}
footer span.bottom-links{float:right;}

