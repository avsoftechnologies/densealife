#body-container .left-body-container .events-heading{background-color:#05a8cf;width:94%;padding:1% 2%;float:left;text-align:left;color:#ffffff;font-weight:bold;font-size: 16px;margin: 10px;}
#body-container .left-body-container .event-slot-aera{width: 100%;margin: 0px auto;padding: 50px 0 0;}
