@charset "utf-8";
body {margin:0px;padding:0px;font-family: 'SegoeUISymbolRegular';font-size:12px;overflow-x:hidden;background-image:url(../img/bg-body.png);background-position:top left;background-repeat:repeat-x;}
/*Comman Styleing*/

@font-face {
    font-family: 'SegoeUISymbolRegular';
    src: url('../fonts/seguisym.eot');
    src: url('../fonts/seguisym.eot') format('embedded-opentype'),
         url('../fonts/seguisym.woff') format('woff'),
         url('../fonts/seguisym.ttf') format('truetype'),
         url('../fonts/seguisym.svg#SegoeUISymbolRegular') format('svg');
}

button{width:40%; float:left;margin:10px;padding:7px 10px;text-align:center;border:1px solid #d8d8d8; background-color:#05a6cc; color:#ffffff;cursor:pointer;}
button.signup{float:none; width:20%;-webkit-box-shadow: 0 0 2px 1px rgba(97,97,97,1);box-shadow: 0 0 2px 1px rgba(97,97,97,1);font-weight:bold;}
button.btn-color{background-color:#23699e;border:1px solid #cccccc;}
label{clear:both;width:70%;float:left;margin:5px 2%;padding:5px 2% 5px 0px;}
input[type=text],  input[type=password]{width:70%;margin:5px 2%;border:1px solid #d8d8d8;line-height:25px; height:25px;padding:5px 2%;-webkit-box-shadow:inset 0 0 20px 5px #eae6e6;box-shadow:inset 0 0 20px 5px #eae6e6;}
input[type=text].inputwidth{width:90%;}
input[type=text]:focus, input[type=password]:focus{background-color:#FFFFFF;-webkit-box-shadow:inset 0 0 20px 5px #ffffff;box-shadow:inset 0 0 20px 5px #ffffff;}
textarea{width:90%;margin:5px 2%;border:1px solid #d8d8d8;line-height:25px; height:75px;padding:5px 2%;-webkit-box-shadow:inset 0 0 20px 5px #eae6e6;box-shadow:inset 0 0 20px 5px #eae6e6;}
.captchawidth{width:49% !important;float:left;}
.captcha-image{width:100px; float:right;margin:7px 25% 0 0;padding:7px 10px;text-align:center;border:1px solid #d8d8d8; background-color:#23699e;border:1px solid #cccccc; color:#ffffff;text-decoration:line-through;}
button.grey{
	color:#044443;
	border:none;
	background: #dddce2; /* Old browsers */
	/* IE9 SVG, needs conditional override of 'filter' to 'none' */
	background: url(data:image/svg+xml;base64,);
	background: -moz-linear-gradient(top,  #dddce2 0%, #cdc9c8 100%); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#dddce2), color-stop(100%,#cdc9c8)); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top,  #dddce2 0%,#cdc9c8 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top,  #dddce2 0%,#cdc9c8 100%); /* Opera 11.10+ */
	background: -ms-linear-gradient(top,  #dddce2 0%,#cdc9c8 100%); /* IE10+ */
	background: linear-gradient(to bottom,  #dddce2 0%,#cdc9c8 100%); /* W3C */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#dddce2', endColorstr='#cdc9c8',GradientType=0 ); /* IE6-8 */
}
a{text-decoration:none;color:#444444;text-shadow: none;}
a:hover{text-decoration:underline;}
ul, ul li, section{margin:0px; padding:0px; list-style-type:none;}
ul li:last-child{border:none;}
ul li img:hover{opacity: 0.8;}
hr{background-color: #e9e9e9;border: none;margin: 10px 0;}
.clearfix:after {visibility: hidden;display: block;font-size: 0;content: " ";clear: both;height: 0;}
.clearfix { display: inline-block; }
/* start commented backslash hack \*/
* html .clearfix { height: 1%; }
.clearfix { display: block; }
