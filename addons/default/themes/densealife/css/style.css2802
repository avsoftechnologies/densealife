#body-container .left-body-container .event-slot-aera ul li .event-slot-image{width:163px; height:230px; background-color:#99FF99;-webkit-box-shadow: 0 0 2px 1px rgba(179,179,179,1);box-shadow: 0 0 2px 1px rgba(179,179,179,1);border:5px solid #ffffff;margin: 0px auto 10px;overflow: hidden;}
